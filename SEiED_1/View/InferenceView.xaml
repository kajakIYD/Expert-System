<UserControl x:Class="SEiED_1.View.InferenceView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SEiED_1.View"
      xmlns:vm="clr-namespace:SEiED_1.ViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800">

    <UserControl.DataContext>
        <vm:MainViewModel></vm:MainViewModel>
    </UserControl.DataContext>

    <Grid>
        <Label Content="Perform inference!" FontSize="20" HorizontalAlignment="Center"/>
        <Button Name="StartButton"  Grid.Row="3" Grid.Column="1" Content="Start process!" Command="{Binding Path=StartProcessCommand}"  HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="Auto"/>
    </Grid>
</UserControl>
